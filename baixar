if lista == 'all':
    elementos = driver.find_elements_by_class_name('attach_filename')
    for elemento in elementos:
        elemento.click()


if lista == 'all':
    for i in range(len(lista)):
        lista[i].find_element_by_class_name('attach_filename').click()



if lista == 'all':
    for i in range(len(lista)):
        webbot.wait_for_element_visibility(element=lista[i], visible=True, waiting_time=10000)
        lista[i].find_element_by_class_name('attach_filename').click()

